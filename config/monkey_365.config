{
    "title": "Monkey 365 Config File",
    "version": {
        "Monkey365Version": "0.91-beta"
    },
    "httpSettings": {
        "userAgent": ""
    },
	"jsonOptions": {
        "splitResources": "false"
    },
    "performance": {
		"BatchSleep": 10000,
		"BatchSize": 50,
        "nestedRunspaces": {
          "MaxQueue": "2"
        }
    },
    "htmlSettings": {
        "tableformat": "core/html/htmlconf/tables",
        "chartformat": "core/html/htmlconf/charts",
        "dashboardformat": "core/html/htmlconf/dashboards"
    },
    "ruleSettings": {
        "rules": "rules/findings/",
        "conditions": "rules/",
        "azureDefaultRuleset": "rules/rulesets/cis_azure_1.4.json",
        "m365DefaultRuleset": "rules/rulesets/cis_m365_1.4.json"
    },
    "logging": {
        "default":[
            {
                "name": "File",
                "type": "file",
                "configuration": {
                    "filename": "monkey365_yyyyMMddhhmmss.log",
                    "includeExceptions": true,
                    "includeDebug": true,
                    "includeVerbose": true,
                    "includeError": true
                }
            }
        ],
        "loggers":[
            {
                "name": "File",
                "type": "file",
                "configuration": {
                    "filename": "monkey365_exceptions_yyyyMMddhhmmss.log",
                    "includeExceptions": true
                }
            }
        ]
    },
    "excelSettings": {
        "ReportName": "Monkey365 Dump Data",
        "HyperLinkcompanyName": "https://silverhack.github.io/monkey365/",
        "CompanyLogo": "images/MonkeyLogo.png",
        "CompanyLogoFront": "images/MonkeyLogo.png",
        "CompanyUserTopLeft": "",
        "dumpformat": "core/office/dumpformat",
        "Debug": "false",
        "tableFormatting": {
          "Style": "TableStyleDark3"
        },
        "HeaderStyle": {
          "1033": "Heading 3",
          "3082": "TÃ­tulo 3"
        }
    },
    "o365": {
        "licenseInfo": "core/utils/licenses/licenses.json",
        "SharePointOnline": {
			"ScanSites": "true",
			"ScanFolders": "false",
			"ScanFiles": "false",
			"Subsites": {
				"Recursive": "true",
				"Depth": "10"
			},
			"SharingLinks": {
				"Include":[
					"Documents"
				]
			},
			"Permissions": {
				"Site":{
					"IncludeInheritedPermissions": "true"
				},
				"Lists":{
					"IncludeInheritedPermissions": "true"
				},
				"Folders":{
					"IncludeInheritedPermissions": "true"
				},
				"Items":{
					"IncludeInheritedPermissions": "true"
				}
			}
        },
        "ExchangeOnline": {
          "GetExchangeGroups": "true",
          "GetPurViewGroups": "false"
        }
    },
    "azuread": {
		"useMsGraph": "true",
		"auditLog":{
			"enabled": "false",
			"AuditLogDaysAgo": "-7"
		},
		"provider": {
			"graph":{
				"api_version": "1.6",
				"internal_api_version": "1.61-internal",
				"getUsersWithAADInternalAPI": "true"
			},
			"portal":{
				"GetManagedApplicationsByPrincipalId": "true"
			},
			"msgraph":{
				"api_version": "V1.0"
			}
		}
    },
