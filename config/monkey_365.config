{
    "title": "Monkey 365 Config File",
     "requirements": {
        "psversion": "3",
        "netversion": "378389"
    },
    "version": {
        "Monkey365Version": "0.7-beta"
    },
    "debug": {
        "showResponseHeaders": "false"
    },
    "httpSettings": {
        "userAgent": ""
    },
	"jsonOptions": {
        "splitResources": "false"
    },
    "performance": {
		"BatchSleep": 4000,
		"BatchSize": 200,
        "nestedRunspaces": {
          "MaxQueue": "2"
        }
    },
    "profileSettings": {
        "profile": "profiles",
        "defaultProfile": "profiles/interactive_all.json"
    },
    "htmlSettings": {
        "tableformat": "core/html/htmlconf/tables"
    },
    "ruleSettings": {
        "rules": "rules/findings/",
        "conditions": "rules/",
        "azureDefaultRuleset": "rules/rulesets/cis_azure_1.4.json",
        "m365DefaultRuleset": "rules/rulesets/cis_m365_1.4.json"
    },
    "logging": {
        "default":[
            {
                "name": "File",
                "type": "file",
                "configuration": {
                    "filename": "monkey365_yyyyMMddhhmmss.log",
                    "includeExceptions": false,
                    "includeDebug": false,
                    "includeVerbose": false,
                    "includeError": false
                }
            }
        ],
        "loggers":[]
    },
    "excelSettings": {
        "ReportName": "Monkey 365 Dump Data",
        "HyperLinkcompanyName": "https://silverhack.github.io/monkey365/",
        "CompanyLogo": "images/MonkeyLogo.png",
        "CompanyLogoFront": "images/MonkeyLogo.png",
        "CompanyUserTopLeft": "",
        "dumpformat": "core/office/dumpformat",
        "Debug": "false",
        "tableFormatting": {
          "Style": "TableStyleDark3"
        },
        "HeaderStyle": {
          "1033": "Heading 3",
          "3082": "TÃ­tulo 3"
        }
    },
    "o365": {
        "licenseInfo": "core/utils/licenses/licenses.json",
        "SharePointOnline": {
			"ScanSites": "true",
			"Subsites": {
				"Recursive": "true",
				"Depth": "10"
			},
			"SitePermissions": {
				"IncludeInheritedPermissions": "true",
				"ScanFolders": "true",
				"ScanFiles": "true"
			}
        },
        "ExchangeOnline": {
          "GetExchangeGroups": "false",
          "GetPurViewGroups": "false"
        }
    },
    "azuread": {
        "usemsalAuth": "true",
        "userFilters": ["*"],
        "DirectoryRolesFilter": ["*"],
        "GroupFilter": ["*"],
        "AuditLogDaysAgo": "-7",
        "GetManagedApplicationsByPrincipalId": "true",
        "GetUserDetails": "true",
        "GetGroupMembers": "true",
        "useAzurePortalAPI": "true",
        "dumpAdUsersWithInternalGraphAPI": "true",
        "api_version": "1.6",
        "internal_api_version": "1.61-internal",
        "AuditLogFilter": [
            "activityType",
            "correlationId",
            "tenantId",
            "activityOperationType",
            "activity",
            "activityDate",
            "actor",
            "actorType",
            "targetResourceType",
            "targetobjectId,targetName",
            "targetUserPrincipalName",
            "ChangeAttribute",
            "OldValue",
            "NewValue"
        ]
    },
